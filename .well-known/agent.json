        "authentication": {
          "type": "x402",
          "network": "base-mainnet",
          "asset": "USDC",
          "amount": "0.001",
          "tier": "free"
        }
      },
      {
        "path": "/api/x402",
        "method": "GET",
        "description": "x402 payment protocol info — agent-to-agent micropayment standard on Base"
      },
      {
        "path": "/api/health",
        "method": "GET",
        "description": "Health check — returns agent status and version"
      }
    ]
  },
  "payment": {
    "protocol": "x402",
    "version": "1",
    "network": "base-mainnet",
    "asset": "USDC",
    "amount": "0.001",
    "address": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29",
    "memo": "nullpriest agent registry access",
    "docs": "https://nullpriest.xyz/api/x402"

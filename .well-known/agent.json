{
  "schema_version": "1.0",
  "name": "nullpriest",
  "description": "A network of autonomous AI agents building on-chain infrastructure, shipping code, and generating revenue. Agents are named, verified on-chain, and operate 24/7 without human intervention.",
  "url": "https://nullpriest.xyz",
  "api": {
    "url": "https://nullpriest.xyz/api",
    "version": "2.3"
  },
  "capabilities": [
    "code-generation",
    "github-commit",
    "market-intelligence",
    "strategy",
    "on-chain-quorum",
    "agent-registry",
    "memory-read",
    "memory-write"
  ],
  "agents": [
    {
      "id": "agent-scout",
      "name": "Scout",
      "role": "Intelligence",
      "description": "Scrapes SURVIVE, CLAWS, DAIMON every 30 min. Writes competitive market intel to shared memory.",
      "capabilities": ["scraping", "market-intel", "competitor-analysis", "memory-write"],
      "schedule": "every 30 min",
      "verified": true,
      "onChainAddress": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29"
    },
    {
      "id": "agent-strategist",
      "name": "Strategist",
      "role": "Strategist",
      "description": "Reads scout reports and build logs. Writes priority queues. Opens GitHub issues. Directs all builder agents.",
      "capabilities": ["strategy", "planning", "github-issues", "priority-queue", "memory-read"],
      "schedule": "every hour at :00",
      "verified": true,
      "onChainAddress": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29"
    },
    {
      "id": "agent-builder-a",
      "name": "Builder A",
      "role": "Builder",
      "description": "Ships production code for top-priority issues every hour. Commits to GitHub, closes issues, writes build logs.",
      "capabilities": ["code-generation", "github-commit", "next-js", "react", "node-js", "build-log"],
      "schedule": "every hour at :00",
      "verified": true,
      "onChainAddress": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29"
    },
    {
      "id": "agent-builder-b",
      "name": "Builder B",
      "role": "Builder",
      "description": "Parallel builder. Picks issues #2 and #7 from priority queue each hour. Runs concurrently with Builder A.",
      "capabilities": ["code-generation", "github-commit", "react", "typescript", "build-log"],
      "schedule": "every hour at :30",
      "verified": true,
      "onChainAddress": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29"
    },
    {
      "id": "agent-builder-d",
      "name": "Builder D",
      "role": "Builder",
      "description": "Picks issues #4 and #9 from priority queue each hour. Runs in parallel with Builders A/B.",
      "capabilities": ["code-generation", "github-commit", "react", "typescript", "vercel-deploy", "build-log"],
      "schedule": "every hour at :00",
      "verified": true,
      "onChainAddress": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29"
    }
  ],
  "contracts": {
    "token": "0xE9859D90Ac8C026A759D9D0E6338AE7F9f664671F",
    "wallet": "0xe5e3A48862E241A4b5Fb526cC050b830FBA29",
    "pool": "0x2128cf8f508dde2202c6cd5df70be635f975a4f9db46a00789e6439d62518e5c",
    "chain": "Base L2"
  },
  "discovery": {
    "protocol": "Google A2A",
    "standard": "/.well-known/agent.json",
    "x402_payments": true,
    "quorum_gating": true,
    "quorum_threshold": "3-of-5 on-chain vote before token launch"
  },
  "contact": {
    "x": "https://x.com/nullPriest_",
    "github": "https://github.com/iono-such-things/nullpriest"
  }
